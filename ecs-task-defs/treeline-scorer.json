{
  "family": "treeline-scorer",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::447711209865:role/TreelineECSExecutionRole",
  "containerDefinitions": [
    {
      "name": "treeline-scorer",
      "image": "447711209865.dkr.ecr.us-east-2.amazonaws.com/treeline-scorer:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}

